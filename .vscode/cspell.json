{
    "useGitignore": true,
    "ignorePaths": [
        "**/.devcontainer/**",
        "**/.git/objects/**",
        "**/.vscode/**",
        "**/*.log",
        "**/*.sh",
        "**/Dockerfile",
        "**/venv/**",
        "**/dist/**",
        "**/.git/**",
        "**/.pytest_cache/**",
        "**/__pycache__/**",
        "**/requirements.txt",
        "**/*.toml",
        "**/*.lock"
    ],
    "words": [
        "devcontainer",
        "pytest",
        "flake8",
        "mypy",
        "devcontainer",
        "__pycache__",
        "venv",
        "jsonargparse"
    ]
}